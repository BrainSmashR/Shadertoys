RendezvousParams =
[[
  float center[2];
  float angle;
  float pivot[2];
  float debug;
  float scale;
  float freq;
  float look[3];
  float look2[3];
  float look3[2];
  float CSize[3];
  float colorrows;
  float colorcontrol;
  float reflections;
  float vignette;
